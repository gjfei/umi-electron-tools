{
  "name": "umi-electron-tools",
  "version": "0.0.1",
  "description": "umi-electron-tools",
  "author": {
    "name": "jarven"
  },
  "private": true,
  "scripts": {
    "clean": "rimraf dist/**",
    "start": "npm run clean && ts-node ./script/start.js",
    "start:web": "cross-env APP_ROOT=src/render umi dev",
    "build": "npm run clean && ts-node ./script/build.js",
    "build:web": "cross-env APP_ROOT=src/render umi build",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/electron/index.js",
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.24",
    "lint-staged": "^10.0.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.24",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-is-dev": "^1.2.0",
    "eslint": "^7.15.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.0",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.2"
  }
}
